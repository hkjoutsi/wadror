<p id="notice"><%= notice %></p>

<p>
  <strong><%= @place.name %></strong><br>
   <%= round(@place.overall.to_d) unless @place.send(:overall).nil? %> points<br>
</p>
<p>
   <%= @place.street %><br>
   <%= @place.zip %> <%= @place.city %> <br>
   <%= @place.country %>
</p>


<div id="map">
<iframe
  width="600"
  height="450"
  frameborder="0" style="border:0"
  src=<%= @mapURL %>>
</iframe>
</div>

<%= link_to 'Search for a place', places_path %>
